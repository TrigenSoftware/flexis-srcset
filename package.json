{
  "name": "@flexis/srcset",
  "version": "1.0.4",
  "description": "Highly customizable lib to generating responsive images.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/flexis-srcset.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-srcset/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "generateDocs": "typedoc ./src --out ./docs --ignoreCompilerErrors --excludeExternals --mode modules; touch docs/.nojekyll",
    "checkDeps": "npm-check -s; exit 0",
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "test": "npm run checkDeps && npm run lint && npm run jest",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "srcset",
    "image",
    "picture",
    "responsive"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.5.5",
    "@types/sharp": "^0.21.0",
    "@types/vinyl": "^2.0.2",
    "css-mediaquery": "^0.1.2",
    "imagemin": "^6.0.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^4.0.0",
    "imagemin-zopfli": "^5.1.0",
    "minimatch": "^3.0.4",
    "sharp": "^0.21.0",
    "vinyl": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@types/jest": "^23.3.1",
    "babel-preset-trigen": "^1.3.0",
    "browserslist-config-trigen": "^1.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "jest": "^23.4.2",
    "npm-check": "^5.7.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-tslint": "^0.1.34",
    "rollup-plugin-typescript2": "^0.17.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-trigen": "^3.0.6",
    "typedoc": "^0.13.0",
    "typescript": "^3.0.1"
  },
  "files": [
    "lib"
  ]
}
